{
    "name": "laminas/laminas-navigation",
    "description": "provides support for managing trees of pointers to web pages",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "navigation"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-navigation/",
        "issues": "https://github.com/laminas/laminas-navigation/issues",
        "source": "https://github.com/laminas/laminas-navigation",
        "rss": "https://github.com/laminas/laminas-navigation/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-config": "self.version",
        "laminas/laminas-mvc": "self.version",
        "laminas/laminas-permissions-acl": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "laminas/laminas-view": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-config": "Laminas\\Config component",
        "laminas/laminas-mvc": "Laminas\\Mvc component",
        "laminas/laminas-permissions-acl": "Laminas\\Permissions\\Acl component",
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component",
        "laminas/laminas-view": "Laminas\\View component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Navigation\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-navigation": "self.version"
    }
}
